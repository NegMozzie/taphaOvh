<!DOCTYPE html>
<html {% block htmlclass %}class=""{% endblock htmlclass %} lang="en">
  <head>

    <meta http-equiv="Content-Type" content="text/html;" charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!--[if IE]>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <![endif]-->
    <title>{% block title %}EdBlog{% endblock %}</title>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    {% block metaTags %}
      {# <meta name="EdBlog">#}
      <meta name="description" content="">
      <meta property="og:site_name" content="EdBlog">
      <meta property="og:url" content="http://www.etondigital.com/">
      <meta property="og:title" content="EdBlog">
      <meta property="og:image" content="{{ asset('bundles/edblog/img/svg/edblog-logo.svg') }}">
      <meta property="og:description" content="EdBlog">
      <meta charset="utf-8">
      {# <meta name="robots" content="noindex,follow" />#}
    {% endblock %}

    {# We merge several css files into 1, and that way we reduce number of calls during page rendering #}
    {# EdBlog css files #}
    {% block stylesheets %}

      {% stylesheets filter="cssrewrite"
       
        'assets/vendor/AdminLTE/bootstrap/css/bootstrap.min.css'
        'assets/vendor/flag-icon-css/css/flag-icon.min.css'
        'assets/vendor/AdminLTE/dist/css/AdminLTE.min.css'
        'assets/vendor/AdminLTE/plugins/iCheck/square/blue.css'
        'assets/vendor/AdminLTE/dist/css/skins/_all-skins.min.css'
        'assets/vendor/AdminLTE/dist/css/skins/_all-skins.min.css'
        'bundles/msportsblog/css/bootstrap.min.css'
        'bundles/msportsblog/css/font-awesome.min.css'
        'bundles/msportsblog/css/animate.min.css'
        'bundles/msportsblog/css/prettyPhoto.css'
        'bundles/msportsblog/css/main.css'
        'bundles/msportsblog/css/responsive.css'

      %}
        <link rel="stylesheet" href="{{ asset_url }}" />
      {% endstylesheets %}
    {% endblock %}

    <link href="{{ asset('/assets/vendor/AdminLTE/bootstrap/css/bootstrap.min.cs') }}" rel="stylesheet">
